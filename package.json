{
  "name": "wagz.wedding",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "build:presta": "presta",
    "build:rollup-dev": "rollup -c --environment=NODE_ENV:development",
    "build:rollup-prod": "rollup -c --environment=NODE_ENV:production",
    "build": "npm-run-all --serial clean build:rollup-prod build:presta",
    "dev:presta": "presta dev",
    "dev:rollup": "rollup -c -w --environment=NODE_ENV:development",
    "dev": "npm-run-all --serial clean build:rollup-dev --parallel dev:*"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.9.2",
    "@sanity/client": "^6.0.1",
    "@selfaware/tailwind-base": "^0.0.7",
    "@studio-freight/lenis": "^1.0.11",
    "algoliasearch": "^4.17.0",
    "choozy": "^1.0.0",
    "focus-trap": "^7.4.0",
    "focus-visible": "^5.2.0",
    "fontfaceobserver": "^2.3.0",
    "gsap": "file:gsap-bonus.tgz",
    "hyposcript": "^1.2.0",
    "lodash.escape": "^4.0.1",
    "martha": "^4.1.0",
    "picoapp": "^3.6.2",
    "quicklink": "^2.3.0",
    "sqzbx": "^0.0.6"
  },
  "devDependencies": {
    "@presta/adapter-netlify": "^0.5.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "cssnano": "^5.0.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-focus-visible": "^6.0.3",
    "postcss-import": "^14.0.2",
    "presta": "^0.45.1",
    "rollup": "^2.66.1",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "tailwindcss": "^3.0.18"
  }
}
